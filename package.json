{
  "name": "livhea_react",
  "version": "0.0.1",
  "private": true,
  "jest": {
    "setupEnvScriptFile": "./node_modules/react-native/jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-addons-test-utils",
      "react-native-router-flux",
      "promise",
      "source-map",
      "key-mirror",
      "immutable",
      "fetch",
      "redux",
      "redux-thunk",
      "fbjs"
    ],
    "collectCoverage": false,
    "verbose": true
  },
  "scripts": {
    "test": "rm -rf ./node_modules/jest-cli/.haste_cache && jest  --no-cache",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test:watch": "npm run test -- --watch",
    "test-chrome": "node-debug --preload false --nodejs --harmony jest --runInBand",
    "docs": "./node_modules/docker/docker -w -i src/ -o ../snowflake-pages "
  },
  "dependencies": {
    "apsl-react-native-button": "^2.5.0",
    "docker": "^0.2.14",
    "immutable": "^3.8.1",
    "key-mirror": "^1.0.1",
    "react-native": "^0.27.2",
    "react-native-gifted-spinner": "0.0.4",
    "react-native-navbar": "^1.5.0",
    "react-native-router-flux": "^3.30.0",
    "react-native-simple-store": "^1.0.1",
    "react-native-simpledialog-android": "^1.0.7",
    "react-native-vector-icons": "^2.0.3",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "regenerator": "^0.8.46",
    "tcomb-form-native": "^0.5.1",
    "underscore": "^1.8.3",
    "validate.js": "^0.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-jest": "^12.1.0",
    "babel-preset-react-native": "^1.9.0",
    "istanbul": "^0.4.3",
    "jest-cli": "^12.1.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "redux-mock-store": "^1.1.0"
  }
}
